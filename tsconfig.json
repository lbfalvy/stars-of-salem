{
  "compilerOptions": {
    "outDir": "./dist/",
    "target": "es5",
    "allowSyntheticDefaultImports" : true,
    "esModuleInterop" : true,
    "allowJs": true,
    "module": "es6",
    "jsx": "react",
    "strict": true,
    "noImplicitAny": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "skipLibCheck": true
  }
}