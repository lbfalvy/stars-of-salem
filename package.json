{
  "name": "stars-of-salem",
  "version": "0.0.2",
  "private": true,
  "description": "My second attempt at diving into the JS ecosystem.",
  "scripts": {
    "debug": "concurrently -n 'server,client' -c 'yellow,cyan' -i 'npm:debug:server' 'npm:debug:client'",
    "debug:client": "webpack serve",
    "debug:server": "nodemon",
    "build-and-run": "npm run build && npm run run",
    "build": "NODE_ENV=production webpack",
    "run": "concurrently -n 'server,client' -c 'yellow,cyan' -i 'npm:run:server' 'npm:run:client'",
    "run:client": "http-server ./dist/ ",
    "run:server": "node dist/server.js",
    "test": "jasmine"
  },
  "author": "Lawrence Bethlenfalvy <lorinc11@gmail.com>",
  "repository": {
    "url": "https://github.com/lbfalvy/stars-of-salem"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/register": "^7.11.5",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.2",
    "@types/express": "^4.17.8",
    "@types/jasmine": "^3.5.14",
    "@types/node": "^14.11.8",
    "@types/react": "^16.9.52",
    "@types/react-dom": "^16.9.8",
    "@types/ws": "^7.2.7",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.3.0",
    "css-loader": "^5.0.0",
    "eslint": "^7.11.0",
    "http-server": "^0.12.3",
    "jasmine": "^3.6.1",
    "nodemon": "^2.0.5",
    "react-refresh": "^0.8.3",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.5",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "webpack": "^5.1.0",
    "webpack-cli": "^4.0.0",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "express": "^4.17.1",
    "node": "^14.13.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "ws": "^7.3.1"
  }
}
